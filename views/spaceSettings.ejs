  <title>Space
  </title>

  <style>
    
    .main-container{
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        margin-top: 60px;
    }
    .settings-container{
        display: flex;
        flex-direction: row;
        flex-wrap: wrap;
        width: 100%;
        justify-content: space-between;
        justify-content: center;
        align-items: center;
    }

    .columnster{
        width: 400px;
        height: 560px;
        max-height: 100vh;
        border-radius: 25px;
        margin: 20px;
        display: flex;
        flex-direction: column;
        align-items: center;
        padding: 10px;
        border: 2px solid blueviolet;
    }

    .waitinglist-container{
        background-color: white;
    }
    .waitinglist-child{
        overflow-y: scroll;
        width: 100%;
        height: 100%;
    }
    .waitinglist-child ul{
        list-style-type: none;
        width: 100%;
    }
    .waitinglist-child ul li{
        display: flex;
        height: 80px;
        width: 100%;
        align-items: center;
        justify-content: space-between;
        padding: 6px;
    }
    .waitinglist-child ul li button{
        height: 30px;
        border: none;
        padding: 2px 10px 2px 10px;
        border-radius: 30px;
        outline: none;
        background-color: blueviolet;
        color: rgb(230, 226, 226);
    }
    .waitinglist-child ul li .waiting-img{
        object-fit: cover;
        border-radius: 50%;
        width: 40px;
        height: 40px;
        overflow: hidden;
        background-color: rgb(112, 112, 112);
    }
    .waiting-img img{
        object-fit: cover;
        width: 100%;
    }
    .waitinglist-child .profile-container{
        display: flex;
        align-items: center;
    }

    .waitinglist-child .profile-container .username{
        font-weight: bold;
        color: black;
        margin-left: 7px;
    }
    .line {
        height: 1px;
        width: 60%;
        display: flex;
        background-color: rgba(0, 0, 0, 0.26);
        margin: auto;
    }

    .inviteusers-container{
        background-color: white;
        /* display: flex;
        justify-content: space-between; */

    }
    .inviteusers-container input{
        width: 90%;
        border: none;
        background-color: transparent;
        outline: none;
    }
    .inviteusers-container .searchtoinvite{
        background-color: rgb(241, 241, 241);
        padding: 2px 8px;
        display: flex;
        align-items: center;
        margin: auto;
        border-radius: 25px;
        justify-content: space-between;
        width: 80%;
    }

    .othersettings-container{
        background-color: white;
    }
    .othersettings-container .other-header{
        display: flex;
        justify-content: space-between;
        align-items: center;
        width: 100%;
        height: 40px;
    }
    .othersettings-container .other-header button{
        height: 30px;
        width: 80px;
        border: none;
        /* padding: 2px 10px 2px 10px; */
        border-radius: 20px;
        outline: none;
        background-color: blueviolet;
        color: rgb(230, 226, 226);
    }

    .othersettings-container form{
        display: flex;
        flex-direction: column;
        width: 100%;
    }
    
    .othersettings-container form .cta{
        margin-top: 20px;
    }
    .successMessage{
      font-size: 20px;
      color: rgb(233, 54, 54);
      font-weight: bold;
      font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;
      margin-top: 10%;
    }

  </style>
  
  
  



  <!-- <script src="/js/chat/updateUI.js"></script> -->
