  <!-- Header START -->
  <header class="navbar-light navbar-sticky">
    <!-- Logo Nav START -->
    <nav class="navbar navbar-expand-xl">
      <div class="container">
        <button class="stack-bars fa ">
        </button>

        <!-- Profile START -->
        <div class="dropdown ms-1 ms-lg-0">

          <personal_info>
            <!-- <account_name><%=user_first_name %> <%= user_last_name %></account_name> -->
          </personal_info>

          <a class="avatar avatar-sm p-0" href="#" id="profileDropdown" role="button" data-bs-auto-close="outside" data-bs-display="static" data-bs-toggle="dropdown" aria-expanded="false">
            <% if(user_profile_picture == "avatar.jpg"){ %>
            <img class="avatar-img rounded-circle border border-white border-3 shadow" src="https://eu.ui-avatars.com/api/?background=random&name=<%= user_first_name %>+<%= user_last_name %>&font-size=0.5&rounded=true&size=128&background=333333&color=ffffff" alt='<%= user_profile_picture %> '>
            <% } else { %>

            <img class="avatar-img rounded-circle border border-white border-3 shadow" src="<%=user_profile_picture%>" alt="profile_img">
            <% }%>

          </a>


          <ul class="dropdown-menu dropdown-animation dropdown-menu-end shadow pt-3" aria-labelledby="profileDropdown">
            <!-- Profile info -->
            <li class="px-3 mb-3">
              <div class="d-flex align-items-center">
                <!-- Avatar -->
                <div class="avatar me-3">
                  <% if(user_profile_picture == "avatar.jpg"){ %>
                  <img class="avatar-img rounded-circle border border-white border-3 shadow" src="https://eu.ui-avatars.com/api/?background=random&name=<%= user_first_name %>+<%= user_last_name %>&font-size=0.5&rounded=true&size=128&background=333333&color=ffffff" alt='<%= user_profile_picture %> '>
                  <% } else { %>

                  <img class="avatar-img rounded-circle border border-white border-3 shadow" src="<%=user_profile_picture%>" alt="profile_img">
                  <% }%>
                </div>
                <div>
                  <a class="h6" href="#">@<%= username %></a>
                  <p class="small m-0"><%= Email %></p>
                </div>
              </div>
            </li>

            <li>
              <hr class="dropdown-divider">
            </li>
            <!-- Links -->
            <li><a class="dropdown-item" href="/settings"><i class="bi bi-person fa-fw me-2"></i>Edit Profile</a></li>
            <li><a class="dropdown-item" href="/settings"><i class="bi bi-gear fa-fw me-2"></i>Account Settings</a></li>
            <li><a class="dropdown-item" href="/contactUs"><i class="bi bi-info-circle fa-fw me-2"></i>Help</a></li>
            <li><a class="dropdown-item bg-danger-soft-hover" href="/logout"><i class="bi bi-power fa-fw me-2"></i>Sign Out</a></li>
            <li>
              <hr class="dropdown-divider">
            </li>
            <!-- Dark mode options START -->
            <li>
              <div class="bg-light dark-mode-switch theme-icon-active d-flex align-items-center p-1 rounded mt-2">
                <button type="button" class="btn btn-sm mb-0" data-bs-theme-value="light">
                  <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-sun fa-fw mode-switch" viewBox="0 0 16 16">
                    <path d="M8 11a3 3 0 1 1 0-6 3 3 0 0 1 0 6zm0 1a4 4 0 1 0 0-8 4 4 0 0 0 0 8zM8 0a.5.5 0 0 1 .5.5v2a.5.5 0 0 1-1 0v-2A.5.5 0 0 1 8 0zm0 13a.5.5 0 0 1 .5.5v2a.5.5 0 0 1-1 0v-2A.5.5 0 0 1 8 13zm8-5a.5.5 0 0 1-.5.5h-2a.5.5 0 0 1 0-1h2a.5.5 0 0 1 .5.5zM3 8a.5.5 0 0 1-.5.5h-2a.5.5 0 0 1 0-1h2A.5.5 0 0 1 3 8zm10.657-5.657a.5.5 0 0 1 0 .707l-1.414 1.415a.5.5 0 1 1-.707-.708l1.414-1.414a.5.5 0 0 1 .707 0zm-9.193 9.193a.5.5 0 0 1 0 .707L3.05 13.657a.5.5 0 0 1-.707-.707l1.414-1.414a.5.5 0 0 1 .707 0zm9.193 2.121a.5.5 0 0 1-.707 0l-1.414-1.414a.5.5 0 0 1 .707-.707l1.414 1.414a.5.5 0 0 1 0 .707zM4.464 4.465a.5.5 0 0 1-.707 0L2.343 3.05a.5.5 0 1 1 .707-.707l1.414 1.414a.5.5 0 0 1 0 .708z" />
                    <use href="#"></use>
                  </svg> Light
                </button>
                <button type="button" class="btn btn-sm mb-0" data-bs-theme-value="dark">
                  <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-moon-stars fa-fw mode-switch" viewBox="0 0 16 16">
                    <path d="M6 .278a.768.768 0 0 1 .08.858 7.208 7.208 0 0 0-.878 3.46c0 4.021 3.278 7.277 7.318 7.277.527 0 1.04-.055 1.533-.16a.787.787 0 0 1 .81.316.733.733 0 0 1-.031.893A8.349 8.349 0 0 1 8.344 16C3.734 16 0 12.286 0 7.71 0 4.266 2.114 1.312 5.124.06A.752.752 0 0 1 6 .278zM4.858 1.311A7.269 7.269 0 0 0 1.025 7.71c0 4.02 3.279 7.276 7.319 7.276a7.316 7.316 0 0 0 5.205-2.162c-.337.042-.68.063-1.029.063-4.61 0-8.343-3.714-8.343-8.29 0-1.167.242-2.278.681-3.286z" />
                    <path d="M10.794 3.148a.217.217 0 0 1 .412 0l.387 1.162c.173.518.579.924 1.097 1.097l1.162.387a.217.217 0 0 1 0 .412l-1.162.387a1.734 1.734 0 0 0-1.097 1.097l-.387 1.162a.217.217 0 0 1-.412 0l-.387-1.162A1.734 1.734 0 0 0 9.31 6.593l-1.162-.387a.217.217 0 0 1 0-.412l1.162-.387a1.734 1.734 0 0 0 1.097-1.097l.387-1.162zM13.863.099a.145.145 0 0 1 .274 0l.258.774c.115.346.386.617.732.732l.774.258a.145.145 0 0 1 0 .274l-.774.258a1.156 1.156 0 0 0-.732.732l-.258.774a.145.145 0 0 1-.274 0l-.258-.774a1.156 1.156 0 0 0-.732-.732l-.774-.258a.145.145 0 0 1 0-.274l.774-.258c.346-.115.617-.386.732-.732L13.863.1z" />
                    <use href="#"></use>
                  </svg> Dark
                </button>
                <button type="button" class="btn btn-sm mb-0 active" data-bs-theme-value="auto">
                  <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-circle-half fa-fw mode-switch" viewBox="0 0 16 16">
                    <path d="M8 15A7 7 0 1 0 8 1v14zm0 1A8 8 0 1 1 8 0a8 8 0 0 1 0 16z" />
                    <use href="#"></use>
                  </svg> Auto
                </button>
              </div>
            </li>
            <!-- Dark mode options END-->
          </ul>
        </div>
        <!-- Profile START -->
      </div>
    </nav>
    <!-- Logo Nav END -->
  </header>
  <!-- Header END -->

  <div id="main-wrapper" class="mainWrapper">

    <!--  Sidebar End -->
    <input type="hidden" name="space_id" id="space_id"  value="<%= spaceId %>" readonly required>
    <div class="main-container">
      <a href="/spaces/<%= spaceId %>" style="color: blueviolet; font-weight: bold;"> <<-- Go back to Space</a>
        <h3>Space Settings</h3>
        <div class="form-control cta" style="width: 60%; display: flex; justify-content: space-between; align-items: center;">
          <label for="passkey">Delete Space:</label>
          <button type="button" class="btn cta" style="border-radius: 10px; background-color: red; color: white; width: 40%;" id="delete_space">Delete</button>
        </div>
        <div class="settings-container">
           <div class="columnster waitinglist-container">
              <h4>Waiting List</h4>
              <div class="waitinglist-child">
                <ul id="waitinglist">
                    
                    
                </ul>
              </div>
             
           </div>
           <div class="columnster inviteusers-container">
             <h4>Invite Users</h4>
             <div class="searchtoinvite">
              <input type="text" class="search-chat" name="" id="" placeholder="Search..."><i class="ti ti-search"></i>
          </div>
             <div class="waitinglist-child">
                
                <ul id="inviteusers" class="invite-users">
                  <li>...fetching users</li>
                  
                    
                </ul>
              </div>
             
           </div>
           <div class="columnster othersettings-container">
              <div class="other-header"><h4>Other Settings</h4> <button type="button" id="space_edit">Edit</button></div>
              <form id="settingsForm">
    <!-- <input type="hidden" name="space_id"  value="<%= spaceId %>" readonly required> -->

                <div class="form-control cta">
                    <label for="space_title">Title:</label>
                    <input class="form-control" type="text" value="<%= SpaceName%>" name="space_title" id="space_title" readonly>
                </div>

                <div class="form-control cta">
                    <label for="space_description">Title Description:</label>
                    <textarea name="space_description" value="<%= SpaceDescription %>" id="space_description" class="form-control" disabled><%= SpaceDescription %></textarea>
                </div>
                <% if(PassKey != null){ %>
                <div class="form-control cta">
                  <label for="passkey">Space Passkey</label>
                  <input class="form-control" type="text" value="<%= PassKey %>" placeholder="<%= PassKey %>">
                </div>
                <% } else { %>
                    <span></span>
                  <%}%>
               
                <div class="form-control cta">
                    <p>Make Private?</p>
                    <label for="makeprivate">Yes</label>
                    <% if(isPrivate === "yes") { %>
                    <input type="radio" name="makeprivate" value="yes" id="yes" checked readonly>
                    <% } else { %>
                      <input type="radio" name="makeprivate" value="yes" id="yes" readonly>
                      <%}%>
                    <label for="makeprivate">No</label>
                    
                    <% if(isPrivate === "no") { %>
                      <input type="radio" name="makeprivate" value="no" id="no" checked readonly>
                      <% } else { %>
                        <input type="radio" name="makeprivate" value="no" id="no" readonly>
                        <%}%>
                </div>

                <button class="btn cta" style="border-radius: 25px; background-color: blueviolet; color: white;" id="submitForm" disabled>Submit</button>

              </form>
              
           </div>
        </div>
       
    </div>
  


  </div>
  <div class="dark-transparent sidebartoggler"></div>
